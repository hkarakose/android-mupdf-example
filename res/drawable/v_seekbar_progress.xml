<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/background">
        <nine-patch android:src="@drawable/v_seekbar_background" android:dither="true"
				/>
    </item>
    <item android:id="@+id/secondaryProgress">
        <clip>
            <shape>
                <gradient android:startColor="#80028ac8" android:endColor="#a004638f" android:angle="270.0" android:centerY="0.75" android:centerColor="#80127fb1" />
                <corners android:radius="5.0dip" />
            </shape>
        </clip>
    </item>
    <item android:id="@+id/progress" android:drawable="@drawable/v_seekbar_progress_bg" />
</layer-list>